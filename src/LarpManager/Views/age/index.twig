{% extends "layout.twig" %}

{% block title %}Ages{% endblock title %}

{% block content %}

<div class="container-fluid">
	<div class="row">
		<div class="col-xs-12 col-md-12">
			<div class="well well-sm">
				<h4>
					Gestion des ages
					<small>
						( {{ ages|length }} ages )
					</small>
				</h4>
			</div>
			{{ include("age/fragment/list.twig", {'ages' : ages, 'add' : true}) }}
		</div>
	</div>

</div>
	
{% endblock %}